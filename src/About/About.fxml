   <children>
      <ImageView fx:id="portrait" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
      <VBox fx:id="gender" prefHeight="200.0" prefWidth="100.0" spacing="20.0">
         <children>
            <Label fx:id="name" text="Name" />
            <Label fx:id="ID" text="ID" />
            <Label layoutX="10.0" layoutY="10.0" text="Gender" />
            <Label fx:id="email" layoutX="10.0" layoutY="10.0" text="Email" />
         </children>
      </VBox>
   </children>
</HBox>
